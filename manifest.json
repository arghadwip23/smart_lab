{
    "name" : "Google Chrome - security",
    "description" :"Chrome security is ment for your better experience. please don't touch it",
    "version":"1.0",
    "manifest_version":3,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js" : ["content.js"],
            "css" :["style.css"]
        }
    ],
    "action":{
        "default_popup":"popup.html",
        "default_title":"Chrome security "
    },
    "permissions": [
    "tabs",
    "activeTab"
  ],
    "icons":{
      "48":"image/chrome.png",
      "128":"image/chrome.png"
    }
}
